{
  "name": "k6-quickpizza-nft-demo",
  "version": "1.0.0",
  "description": "A k6 quickpizza api NFT demo",
  "main": "./load_tests/multiple-spike-scenarios.js",
  "scripts": {
    "test": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=html-report.html K6_PROMETHEUS_RW_TREND_STATS=p\\(95\\),p\\(99\\),min,max k6 run --out=experimental-prometheus-rw --summary-trend-stats min,avg,med,max,p\\(95\\),p\\(99\\),p\\(99.99\\) ./load_tests/multiple-spike-scenarios.js -e ENVIRONMENT=test",
    "dev": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=html-report.html K6_PROMETHEUS_RW_TREND_STATS=p\\(95\\),p\\(99\\),min,max k6 run --out=experimental-prometheus-rw --summary-trend-stats min,avg,med,max,p\\(95\\),p\\(99\\),p\\(99.99\\) ./load_tests/multiple-spike-scenarios.js -e ENVIRONMENT=dev",
    "prod": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=html-report.html K6_PROMETHEUS_RW_TREND_STATS=p\\(95\\),p\\(99\\),min,max k6 run --out=experimental-prometheus-rw --summary-trend-stats min,avg,med,max,p\\(95\\),p\\(99\\),p\\(99.99\\) ./load_tests/multiple-spike-scenarios.js -e ENVIRONMENT=dev",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmaprc/k6-quickpizza-nft-poc.git"
  },
  "keywords": [
    "k6",
    "quickpizza",
    "NFT"
  ],
  "author": "cosmaprc@gmail.com",
  "bugs": {
    "url": "https://github.com/cosmaprc/k6-quickpizza-nft-poc/issues"
  },
  "homepage": "https://github.com/cosmaprc/k6-quickpizza-nft-poc/tree/main#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@eslint/markdown": "^6.4.0",
    "eslint": "^9.25.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "3.5.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
